alias ls="ls -F"
alias ll="ls -l"
alias lst="ls -lrth"
alias cls="clear; ls"
alias ..="cd .."
alias gs="git status"
alias ga="git add"
alias gp="git commit -m up; git push"

PATH=$PATH:$HOME/bin:$HOME/Code/bin
export PYTHONPATH=$PYTHONPATH:$HOME/lib:$HOME/Code/lib
export PERL5LIB=$PERL5LIB:$HOME/lib:$HOME/Code/lib
